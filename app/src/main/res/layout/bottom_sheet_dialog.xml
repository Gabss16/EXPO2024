<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/relativeLayout"
        android:layout_width="413dp"
        android:layout_height="343dp"
        android:layout_margin="2dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-3dp">

        <ScrollView
            android:layout_width="413dp"
            android:layout_height="343dp"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="1dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <EditText
                    android:id="@+id/editTextText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edittext_trabajos"
                    android:ems="10"
                    android:hint="Título"
                    android:inputType="text"
                    android:paddingLeft="35dp"
                    android:textColor="#97000000"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.505"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.1" />

                <EditText
                    android:id="@+id/editTextText1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edittext_trabajos"
                    android:ems="10"
                    android:hint="Ubicación"
                    android:inputType="text"
                    android:paddingLeft="35dp"
                    android:textColor="#97000000"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.494"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.4" />

                <EditText
                    android:id="@+id/editTextText2"
                    android:layout_width="325dp"
                    android:layout_height="66dp"
                    android:background="@drawable/edittext_trabajos"
                    android:ems="10"
                    android:hint="Descripción"
                    android:inputType="text"
                    android:paddingLeft="35dp"
                    android:textColor="#97000000"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.494"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.24" />

                <EditText
                    android:id="@+id/editTextText3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edittext_trabajos"
                    android:ems="10"
                    android:hint="Experiencia Requerida"
                    android:inputType="text"
                    android:paddingLeft="35dp"
                    android:textColor="#97000000"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.494"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.53" />

                <Button
                    android:id="@+id/button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="508dp"
                    android:background="@drawable/boton_publicar_trabajo"
                    android:foreground="@drawable/boton_publicar_trabajo"
                    android:text="Button"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:layout_editor_absoluteX="91dp" />

                <EditText
                    android:id="@+id/editTextText4"
                    android:layout_width="325dp"
                    android:layout_height="66dp"
                    android:background="@drawable/edittext_trabajos"
                    android:ems="10"
                    android:hint="Habilidades Y Competencias"
                    android:inputType="text"
                    android:paddingLeft="35dp"
                    android:textColor="#97000000"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.494"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.7" />

                <EditText
                    android:id="@+id/editTextText5"
                    android:layout_width="325dp"
                    android:layout_height="66dp"
                    android:background="@drawable/edittext_trabajos"
                    android:ems="10"
                    android:hint="Beneficios"
                    android:inputType="text"
                    android:paddingLeft="35dp"
                    android:textColor="#97000000"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.494"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.7" />

                <EditText
                    android:id="@+id/editTextText3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edittext_trabajos"
                    android:ems="10"
                    android:hint="Salario"
                    android:inputType="text"
                    android:paddingLeft="35dp"
                    android:textColor="#97000000"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.494"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.85" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>